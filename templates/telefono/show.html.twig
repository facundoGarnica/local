{% extends 'base.html.twig' %}

{% block title %}Telefono{% endblock %}

{% block body %}
    <h1>Mostrar Teléfonos</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>ID</th>
                <td>{{ telefono.id }}</td>
            </tr>
            <tr>
                <th>Número</th>
                <td>{{ telefono.numero }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_telefono_index') }}">Regresar</a>

    <a href="{{ path('app_telefono_edit', {'id': telefono.id}) }}">Editar</a>

    {{ include('telefono/_delete_form.html.twig') }}
{% endblock %}
